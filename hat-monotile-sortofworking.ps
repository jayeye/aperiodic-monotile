%!

/printtomark {
    1 dict begin
    /points exch def
    gsave
    {
        2 copy
        moveto
        3 -1 roll
        show
        ( ) show
        points
        {
            exch 40 string cvs show
            (, ) show
            40 string cvs show
        }
        { pop pop }
        ifelse
        counttomark 0 eq {
            exit
        } if
    } loop
    pop
    grestore
} def

/v4 {                           % drawline_proc fill_proc >>> --
    4 dict begin
    /fillproc exch def
    /drawlineproc exch def
    /s2 3 sqrt 2 div def        % sin(60)
    /s4 s2 2 div def

    currentpoint
    newpath
        moveto                  % our new (0, 0)
        mark
        (A) currentpoint 3 copy == == == ( ) ==
        .5 0 rlineto
        (B) currentpoint 3 copy == == == ( ) ==
        0 s2 rlineto
        (C) currentpoint 3 copy == == == ( ) ==
        .75 s4 rlineto
        (D) currentpoint 3 copy == == == ( ) ==
        .25 s4 neg rlineto
        (E) currentpoint 3 copy == == == ( ) ==
        .5 0  rlineto
        (F) currentpoint 3 copy == == == ( ) ==
        0 s2 neg rlineto
        (G) currentpoint 3 copy == == == ( ) ==
        .75 s4 neg rlineto
        (H) currentpoint 3 copy == == == ( ) ==
        -.25 s4 neg rlineto
        (I) currentpoint 3 copy == == == ( ) ==
        -1 0 rlineto
        (J) currentpoint 3 copy == == == ( ) ==
        -.25 s4 rlineto
        (K) currentpoint 3 copy == == == ( ) ==
        -.75 s4 neg rlineto
        (L) currentpoint 3 copy == == == ( ) ==
        -.75 s4 rlineto
        (M) currentpoint 3 copy == == == ( ) ==
    closepath
    gsave
        fillproc
    grestore
    gsave
        drawlineproc
    grestore
    false printtomark
    end
} def


/scalefactor 60 def

/Helvetica findfont 18 scalefactor div scalefont setfont
% /Helvetica findfont 18 scalefont setfont

