    3 1 roll                    % ½*x-√3/2y x y
    exch sq3 mul add half mul   % ½*x-√3/2y √3/2x+½*y
    2 array astore
} def

/vrot-60 {                      % [x y] >>> [½*x+√3/2y √3/2x-½*y]
    aload pop                   % x y
